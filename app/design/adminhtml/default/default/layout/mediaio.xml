<?xml version="1.0"?>
<layout version="1.0">

    <mediaio_adminhtml_media_export_index>
        <reference name="head">
            <action method="addJs"><script>mediaio/jquery-3.6.0.min.js</script></action>
            <action method="addJs"><script>mediaio/handle-export.js</script></action>
            <action method="addJs"><script>mediaio/load-templates.js</script></action>
        </reference>
        <reference name="content">
            <block type="core/template" name="mediaio_export" template="mediaio/products/export.phtml" />
        </reference>
    </mediaio_adminhtml_media_export_index>

    <mediaio_adminhtml_media_update_index>
        <reference name="head">
            <action method="addJs"><script>mediaio/jquery-3.6.0.min.js</script></action>
            <action method="addJs"><script>mediaio/handle-import.js</script></action>
            <action method="addJs"><script>mediaio/load-templates.js</script></action>
        </reference>
        <reference name="content">
            <block type="core/template" name="mediaio_update" template="mediaio/products/update.phtml" />
        </reference>
    </mediaio_adminhtml_media_update_index>

</layout>